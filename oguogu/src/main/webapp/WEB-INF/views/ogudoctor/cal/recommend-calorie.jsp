<script>
	$(document).ready(function() {
		init();
	});

	function init() {
		$jsp = $('.recommendCalorieJsp');
		$jsp.find('.sc-dsInJy').on(
				'click',
				function() {
					$jsp.find('.sc-dsInJy').removeClass('liyBtK').addClass(
							'iempoK');
					if ($(this).find('.sc-YtoFE').text() == '고양이'
							&& $(this).hasClass('liyBtK')) {
						$(this).removeClass('liyBtK').addClass('iempoK');
					} else if ($(this).find('.sc-YtoFE').text() == '고양이'
							&& $(this).hasClass('iempoK')) {
						$(this).removeClass('iempoK').addClass('liyBtK');
					}
					if ($(this).find('.sc-YtoFE').text() == '강아지'
							&& $(this).hasClass('liyBtK')) {
						$(this).removeClass('liyBtK').addClass('iempoK');
					} else if ($(this).find('.sc-YtoFE').text() == '강아지'
							&& $(this).hasClass('iempoK')) {
						$(this).removeClass('iempoK').addClass('liyBtK');
					}
				});

		$jsp.find('.sc-FIATH').on(
				'focus',
				function() {
					$(this).closest('.sc-gtIISA').removeClass('jpeBZh')
							.addClass('oLOis');
				});

		$jsp.find('.sc-FIATH').on(
				'blur',
				function() {
					$(this).closest('.sc-gtIISA').removeClass('oLOis')
							.addClass('jpeBZh');
				});

		$jsp
				.find('.sc-fYaxgZ')
				.on(
						'click',
						function() {
							var selectVal = '';
							var $selectWrap = $jsp.find('.sc-fGjrnr').find(
									'.sc-dsInJy');
							$selectWrap
									.each(function(idx, inp) {
										console.log($(inp).find('.sc-YtoFE')
												.text());
										if ($(inp).find('.sc-YtoFE').text() == '고양이'
												&& $(inp).hasClass('liyBtK')) {
											selectVal = '고양이';
										} else if ($(inp).find('.sc-YtoFE')
												.text() == '강아지'
												&& $(inp).hasClass('liyBtK')) {
											selectVal = '강아지';
										}
									});
							var option = selectVal == '강아지' ? 'dogOption'
									: 'catOption';
							if (selectVal != '') {
								if ($(this).hasClass('bOQCgT')) {
									$(this).removeClass('bOQCgT').addClass(
											'hkdfjs');
									$jsp.find('.' + option).removeClass(
											'fGKoZE').addClass('faILXv');
								} else {
									$(this).removeClass('hkdfjs').addClass(
											'bOQCgT');
									$jsp.find('.' + option).removeClass(
											'faILXv').addClass('fGKoZE');
								}
							}
						});

		$jsp.find('.sc-gloVsK').on('click', function() {
			var text = $(this).find('.sc-fmXJyp').text();
			$jsp.find('.petStatus').html(text);
			$jsp.find('.sc-fmXJyp').removeClass('hSpSRh').addClass('gJzIou');
			$jsp.find('.sc-gAfzvj').removeClass('faILXv').addClass('fGKoZE');
			$('.sc-fYaxgZ').removeClass('hkdfjs').addClass('bOQCgT');
			$jsp.find('.getResult').trigger('click');
		});

		$('#insWeight').on('input', function() {
			var inputValue = this.value;
			var pattern = /^\d+(\.\d{0,2})?$/;

			if (!pattern.test(inputValue)) {
				// 입력값이 유효하지 않을 때, 이전 유효한 값으로 복원
				this.value = this.getAttribute("data-previous-value") || "";
			} else {
				// 입력값이 유효할 때, 현재 값을 이전 유효한 값으로 설정
				this.setAttribute("data-previous-value", inputValue);
			}
		});

		// 결과보기 버튼 이벤트
		$jsp.find('.getResult').on('click', function() {
			var data = getValue();
			var result = calculateDogRecommendedCalories(data);
			$jsp.find('.dayCal').html(result.dayCal);
			$jsp.find('.basCal').html(result.basCal);
			$jsp.find('.eHwBqx').show();
			//location.replace('/recommend_res.do')
		});

		//입력값 받아오기
		function getValue() {
			var pet = '';
			var $petGroup = $jsp.find('.sc-YtoFE'); // find => 전체에서 찾는
			$petGroup.each(function(idx, inp) {
				if ($(inp).text() == '고양이'
						&& $(inp).closest('.sc-dsInJy').hasClass('liyBtK')) { // closest => 
					pet = '고양이';
				} else if ($(inp).text() == '강아지'
						&& $(inp).closest('.sc-dsInJy').hasClass('liyBtK')) {
					pet = '강아지';
				}
			});
			var weight = Number($jsp.find('#insWeight').val());
			var petStatus = $jsp.find('.petStatus').text();
			var activityCoefficients = getActivityCoefficients(pet, petStatus);
			var result = {
				'pet' : pet,
				'weight' : weight,
				'activityCoefficients' : activityCoefficients
			};
			return result;
		}
		;

		function getActivityCoefficients(pet, petStatus) {
			var option = {
				'강아지' : {
					'성장기 (4개월미만)' : 3,
					'성장기 (4-12개월)' : 2,
					'미중성 성견' : 1.8,
					'중성화 완료 성견' : 1.6,
					'체중 감량 필요 성견' : 1,
					'체중 증량 필요 성견' : 1.7,
					'가벼운 활동량의 사역견' : 2,
					'중간 활동량의 사역견' : 3,
					'높은 활동량의 사역견' : 6
				},
				'고양이' : {
					'유아기 (4개월미만)' : 3,
					'고속 성장기 (4-6개월)' : 2.5,
					'고속 성장기 (7-12개월)' : 2,
					'중성화 완료 성묘' : 1.2,
					'일반 성묘' : 1.4,
					'체중 감량 필요 성묘' : 0.8,
					'운동량이 많은 성묘' : 1.6,
					'노묘 (n세 이상)' : 0.7
				}
			};
			return option[pet][petStatus];
		}

		// 강아지 권장칼로리 계산
		function calculateDogRecommendedCalories(data) {
			var dayCalories = 70 * (data.weight * 0.75);
			var recommendedCalories = (70 * (data.weight * 0.75))
					* data.activityCoefficients;
			return {'dayCal' : dayCalories.toFixed(2), 'basCal' : recommendedCalories.toFixed(2)};
		}
		;

	};
</script>
</html>