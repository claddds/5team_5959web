<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="oguself">
	<!-- meal-식사량 리스트 뽑기 -->
	<select id="dogmealfoodlist" resultType="oguselfvo">
		select * 
		from ogudoctor_disease
		where sym_chk = "식사량"
	</select>
	
	<!-- meal-식수량 리스트 뽑기 -->
	<select id="dogmealwaterlist" resultType="oguselfvo">
		select * 
		from ogudoctor_disease
		where sym_chk = "음수량"
	</select>
	
</mapper>